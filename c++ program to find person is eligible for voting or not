#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
int age;
cout<<"Enter your age:";
cin>>age;
//person is teenager or not
//>=13 and <=19
if(age>=13 && age<=19)
{
	cout<<"Person is Teenager"<<endl;	
}
{
	cout<<"Person is not Teenager"<<endl;
}
//condition to check voting eligiblity
if(age>=18)
{
	cout<<"Person is eligible for voting"<<endl;
}
else
{
	cout<<"Person is not eligible for voting"<<endl;
}
getch();

}
